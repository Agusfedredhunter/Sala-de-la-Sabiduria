-- Consultas con Join

select
 b.nombre,
 b.cuota
from
 barcos b
join
 socios s on b.id_socio=s.id_socio
where
 s.nombre = 'juan perez';

-- Consultas sin Join

select 
 nombre
from
 barcos
where 
 matricula in (
         select
          matricula
		from
         salidas
         where
         destino='mallorca'
);
